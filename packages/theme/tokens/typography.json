{
  "textStyle": {
    "$type": "typography",
    "xs": {
      "$value": {
        "fontFamily": "ui-sans-serif",
        "fontSize": "{size.3}",
        "lineHeight": "{size.4}"
      }
    },
    "sm": {
      "$value": {
        "fontFamily": "ui-sans-serif",
        "fontSize": "{size.3-5}",
        "lineHeight": "{size.5}"
      }
    },
    "base": {
      "$value": {
        "fontFamily": "ui-sans-serif",
        "fontSize": "{size.4}",
        "lineHeight": "{size.6}"
      }
    },
    "lg": {
      "$value": {
        "fontFamily": "ui-sans-serif",
        "fontSize": "{size.4-5}",
        "lineHeight": "{size.7}"
      }
    },
    "xl": {
      "$value": {
        "fontFamily": "ui-sans-serif",
        "fontSize": "{size.5}",
        "lineHeight": "{size.7}"
      }
    },
    "2xl": {
      "$value": {
        "fontFamily": "ui-sans-serif",
        "fontSize": "1.5rem",
        "lineHeight": "2rem"
      }
    }
  },
  "fontWeight": {
    "$type": "fontWeight",
    "light": {
      "$value": 300
    },
    "normal": {
      "$value": 400
    },
    "medium": {
      "$value": 500
    },
    "semi-bold": {
      "$value": 600
    },
    "bold": {
      "$value": 700
    }
  }
}
