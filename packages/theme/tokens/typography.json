{
  "textStyle": {
    "$type": "typography",
    "xs": {
      "$value": {
        "fontFamily": "ui-sans-serif",
        "fontSize": "0.75rem",
        "lineHeight": "1rem"
      }
    },
    "sm": {
      "$value": {
        "fontFamily": "ui-sans-serif",
        "fontSize": "0.875rem",
        "lineHeight": "1.25rem"
      }
    },
    "base": {
      "$value": {
        "fontFamily": "ui-sans-serif",
        "fontSize": "1rem",
        "lineHeight": "1.5rem"
      }
    },
    "lg": {
      "$value": {
        "fontFamily": "ui-sans-serif",
        "fontSize": "1.125rem",
        "lineHeight": "1.75rem"
      }
    },
    "xl": {
      "$value": {
        "fontFamily": "ui-sans-serif",
        "fontSize": "1.25rem",
        "lineHeight": "1.75rem"
      }
    },
    "2xl": {
      "$value": {
        "fontFamily": "ui-sans-serif",
        "fontSize": "1.5rem",
        "lineHeight": "2rem"
      }
    }
  },
  "fontWeight": {
    "$type": "fontWeight",
    "light": {
      "$value": 300
    },
    "normal": {
      "$value": 400
    },
    "medium": {
      "$value": 500
    },
    "semi-bold": {
      "$value": 600
    },
    "bold": {
      "$value": 700
    }
  }
}
